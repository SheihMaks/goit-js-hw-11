{{#each this}}<div class="photo-card">
  <img src="{{webformatURL}}" alt="picture"  loading="lazy" />
  <div class="info">
    <p class="info-item">
      <b>Likes:{{likes}}</b>
    </p>
    <p class="info-item">
      <b>views:{{views}}</b>
    </p>
    <p class="info-item">
      <b>Comments:{{comments}}</b>
    </p>
    <p class="info-item">
      <b>Downloads:{{downloads}}</b>
    </p>
  </div>
</div>{{/each}}